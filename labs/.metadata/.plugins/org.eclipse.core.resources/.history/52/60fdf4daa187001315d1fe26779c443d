#include <iostream>
#include <set>
#include <string>
#include <fstream>
using namespace std;

set<string> newDict(char* from){
	ifstream src;
	src.open(from);

	set<string> dest;
	string s;
	while(getline(from, s);){
		dest.insert(s);
	}

	return dest;
}

int main(int argc, char* argv[]) {

	set<string> dictionary = newDict(argv[1]);

	for(auto printable : dictionary){
		cout << printable.c_str() << endl;
	}

	return 0;
}
